{
  "name": "promociones",
  "version": "0.0.1",
  "description": "Microservicio de promociones y cupones ",
  "keywords": [
    "loopback-application",
    "loopback",
    "LoopBack",
    "example",
    "tutorial",
    "CRUD",
    "models",
    "todo",
    "jwt",
    "authentication"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10.16"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "npm run eslint && npm run prettier:check",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run rebuild",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t promociones .",
    "docker:run": "docker run -p 3000:3000 -d promociones",
    "premigrate": "npm run build",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "npm run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "npm run rebuild",
    "start": "node -r source-map-support/register .",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "rebuild": "npm run clean && npm run build",
    "heroku-run-build-script": "true",
    "postinstall": "npm run rebuild",
    "start:prod": "node dist/index.js",
    "verify": "npm pack && tar xf loopback-todo*.tgz && tree package && npm run clean"
  },
  "repository": {
    "type": "git",
    "url": "",
    "directory": ""
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/boot": "^3.4.3",
    "@loopback/core": "^2.17.0",
    "@loopback/repository": "^3.7.2",
    "@loopback/rest": "^10.0.1",
    "@loopback/rest-explorer": "^3.3.3",
    "@loopback/service-proxy": "^3.2.3",
    "loopback-connector-mongodb": "^5.6.0",
    "loopback-connector-rest": "^3.7.0",
    "tslib": "^2.0.0",
    "@loopback/authentication": "^7.1.0",
    "@loopback/authentication-jwt": "^0.8.0",
    "@loopback/security": "^0.4.0",
    "@types/bcryptjs": "^2.4.2",
    "bcryptjs": "^2.4.3"
  },
  "devDependencies": {
    "@loopback/build": "^7.0.1",
    "source-map-support": "^0.5.20",
    "@loopback/testlab": "^3.4.3",
    "@types/node": "^10.17.60",
    "@loopback/eslint-config": "^11.0.1",
    "eslint": "^7.32.0",
    "typescript": "~4.4.4",
    "@loopback/http-caching-proxy": "^2.2.0",
    "@types/lodash": "^4.14.168",
    "lodash": "^4.17.21"
  }
}
